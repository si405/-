<h1>Edit artist: <%= @artist.artist_name %></h1>

<%= form_for @artist do |f| %>
	<p>
    	<%= f.label :artist_name %><br>
   		<%= f.text_field :artist_name %>
 	</p>
 
  <p>
  	 <%= f.label :artist_age %><br>
  	 <%= f.text_field :artist_age %>
  </p>
 
 <p>
  	<%= f.label :label_id %><br>
  	<%= f.text_field :label_id %>
  </p>
 	<p>
   	<%= f.submit %>
 	</p>
<% end %>

<%= link_to 'View all artists', artists_path %>