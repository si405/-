
<% binding.pry %>

<%= form_for ([@artist, @song]) do |f| %>
    <p>
        <%= f.label :song_name %><br>
        <%= f.text_field :song_name %>
    </p>
    <p>
        <%= f.label :genre %><br>
        <%= f.text_field :genre %>
    </p>
    <p>
        <%= f.label :duration %><br>
        <%= f.text_field :duration %>
    </p>
    <p>
        <%= f.label :release_date %><br>
        <%= f.date_field :release_date %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>

